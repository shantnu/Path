        <section class="wow fadeIn">
            <div class="container">
                <div class="row">
                    <div class="col-md-10 col-sm-12 col-xs-12 center-col xs-text-center">
                            <%= yield %>
                    </div>

            </div>
        </section>
        <!-- end section -->
        <!-- start parallax section -->
        <section class="wow fadeIn no-padding parallax one-fourth-screen sm-height-500px xs-height-350px background-position-x-50" data-stellar-background-ratio="0.5" style="background-image: url('images/sea.jpg')">
