                            <h1 class="text-white alt-font font-weight-600 margin-10px-bottom">THE PATH TO JOY</h1>
                            <!-- end page title -->
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <!-- end page title section -->
        <!-- start information section -->
        
        <!-- end parallax section -->
